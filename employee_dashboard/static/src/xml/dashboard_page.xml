          crossorigin="anonymous"></script>
  
        <div class="wrapper">
          <t t-call="employee_dashboard.sidebar_template"/>
          <div class="grey-bg container-fluid px-5 template-wrapper">
            <t t-call="employee_dashboard.dashboard_new_template" />
          </div>
